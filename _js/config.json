{
  "ui": [
    {
      "selector": ".index .blog p:first",
      "use": "reduce"
    },
    {
      "selector": ".index .blog p:first",
      "use": "elipsis",
      "config": {
        "words": -4
      }
    },
    {
      "selector": ".post .blog a[href*='gist.github.com']",
      "use": "gist"
    },
    {
      "selector": ".post .blog img[src*='googleusercontent.com']",
      "use": "googleimagesize"
    }
  ]
}
